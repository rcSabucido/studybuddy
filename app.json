{
  "expo": {
    "name": "studybuddy",
    "slug": "studybuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sb-icon.png",
    "scheme": "studybuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.studybuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sb-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.anonymous.studybuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/sb-splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-font",
        {}
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d8b25316-4b3e-43ca-bf19-cb47c17f18e8"
      }
    }
  }
}
